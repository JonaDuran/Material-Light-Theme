		"editor.foreground": "#E0E0E0",
		
		"editorCursor.foreground": "#E0E0E0",
		"editor.lineHighlightBackground": "#383838",
		"editor.selectionBackground": "#485060",
		"editor.wordHighlightBackground": "#484848",
		"editor.wordHighlightStrongBackground": "#485060",
		"editor.findMatchHighlightBackground": "#ffd18038",
