		"activityBarBadge.foreground": "#E0E0E0",
		"activityBarBadge.background": "#1565c0",
		"focusBorder": "#1565c0",
		
		"statusBar.background": "#181818",
		"statusBar.foreground": "#E0E0E0",

		"editorOverviewRuler.bracketMatchForeground": "#ea80fc88",
		"editorOverviewRuler.wordHighlightForeground": "#ea80fc88",
		"editorOverviewRuler.wordHighlightStrongForeground": "#ea80fc",
		"editorOverviewRuler.addedForeground": "#8bc34a88",
		"editorGutter.addedBackground": "#8bc34a88",
		"editorOverviewRuler.modifiedForeground": "#40c4ff88",
		"editorGutter.modifiedBackground": "#40c4ff88",
		"editorOverviewRuler.deletedForeground": "#e5737388",
		"editorGutter.deletedBackground": "#e5737388",

		"input.background": "#383838",
		"input.foreground": "#E0E0E0",
		
		"terminal.foreground": "#E0E0E0",
		"terminal.ansiBlack": "#212121",
		"terminal.ansiBlue": "#40c4ff",
		"terminal.ansiGreen": "#8bc34a",
		"terminal.ansiYellow": "#ffd180",
		"terminal.ansiCyan": "#00b8d4",
		"terminal.ansiMagenta": "#ea80fc",
		"terminal.ansiRed": "#e57373",
		"terminal.ansiWhite": "#d7dae0",
		"terminal.ansiBrightBlack": "#E0E0E0",
		"terminal.ansiBrightBlue": "#40c4ff",
		"terminal.ansiBrightGreen": "#8bc34a",
		"terminal.ansiBrightYellow": "#ffd180",
		"terminal.ansiBrightCyan": "#00b8d4",
		"terminal.ansiBrightMagenta": "#ea80fc",
		"terminal.ansiBrightRed": "#e57373",
		"terminal.ansiBrightWhite": "#d7dae0",
	},
	"tokenColors": [
		{
			"scope": [
				"string",
				"constant.other.character-class.range.regexp",
				"constant.other.character-class.set.regexp",
				"keyword.operator.quantifier.regexp",
				"punctuation.definition.template-expression",
				"variable.interpolation.scss",
				"support.constant.property-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ffd180"
			}
		},
		{
			"scope": [
				"variable",
				"meta.template.expression",
				"constant.language.pseudo",
				"punctuation.separator.key-value",
				"storage.modifier.import",
				"new.expr entity.name.type.module",
				"constant.other.php",
				"support.variable"
			],
			"settings": {
				"foreground": "#E0E0E0"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.numeric",
				"constant.other.color",
				"variable.language"
			],
			"settings": {
				"foreground": "#e57373"
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"entity.name.tag.reference",
				"punctuation.separator.key-value",
				"support.type.python"
			],
			"settings": {
				"foreground": "#ea80fc"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.function",
				"support.function",
				"support.class.builtin",
				"entity.name.type.class",
				"new.expr entity.name.type",
				"storage.type.java",
				"punctuation.definition.tag",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#40c4ff"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"meta.object-literal.key",
				"variable.object.property",
				"support.type.property-name.json",
				"entity.name.tag.css",
				"entity.name.tag.custom",
				"variable.parameter.function-call"
			],
			"settings": {
				"foreground": "#8bc34a"
			}
		},
		{
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc",
				"comment.block.documentation.phpdoc",
				"string.quoted.docstring.multi"
			],
			"settings": {
				"foreground": "#787878",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.type.annotation",
				"meta.return.type",
				"keyword.operator.type.annotation",
				"meta.type.parameters",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#78909C"
			}
		},
		{
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#e57373",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.raw",
				"markup.inline.raw"
			],
			"settings": {
				"foreground": "#ffd180"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"fontStyle": "bold"
			}
		}
	]
}