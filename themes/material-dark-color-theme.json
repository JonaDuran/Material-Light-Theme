		"editor.hoverHighlightBackground": "#484848",
		"editorBracketMatch.background": "#485060",
		"editorLineNumber.foreground": "#787878",
		
		"editorIndentGuide.activeBackground": "#686868",
		"editorIndentGuide.background": "#484848",
		
		"editorWidget.background": "#202020",
