{
	"type": "light",
	"colors": {
		"editor.background": "#FAFAFA",
		"editor.foreground": "#212121",
		"editor.lineHighlightBackground": "#01579B11",
		"editor.selectionBackground": "#01579B22",
		"editorCursor.foreground": "#212121",
		"editor.wordHighlightBackground": "#01579B11",
		"editor.wordHighlightStrongBackground": "#01579B22",
		"editor.findMatchHighlightBackground": "#FFA00033",
		"editor.findMatchBackground": "#FFA00077",
		"editor.hoverHighlightBackground": "#01579B22",
		"editorBracketMatch.background": "#01579B22",
		"editorWhitespace.foreground": "#E0E0E0",
		"editorLineNumber.foreground": "#9E9E9E",
		"editorSuggestWidget.background": "#F5F5F5",
		
		"tab.inactiveBackground": "#F5F5F5",
		"sideBar.foreground": "#212121",
		"sideBar.background": "#F5F5F5",
		
		"list.hoverBackground": "#01579B11",
		"list.inactiveSelectionBackground": "#01579B22",
		"list.activeSelectionBackground": "#039BE5",
		"list.focusBackground": "#0288D122",
		
		"editorOverviewRuler.errorForeground": "#F44336",
		"editorOverviewRuler.warningForeground": "#FFC107",
		"editorOverviewRuler.wordHighlightForeground": "#AC00C088",
		"editorOverviewRuler.wordHighlightStrongForeground": "#AC00C0",
		"editorOverviewRuler.addedForeground": "#2E7D3288",
		"editorGutter.addedBackground": "#2E7D3288",
		"editorOverviewRuler.modifiedForeground": "#1565C088",
		"editorGutter.modifiedBackground": "#1565C088",
		"editorOverviewRuler.deletedForeground": "#C6282888",
		"editorGutter.deletedBackground": "#C6282888"
	},
	"tokenColors": [
		{
			"scope": [
				"string",
				"constant.other.character-class.range.regexp",
				"constant.other.character-class.set.regexp",
				"keyword.operator.quantifier.regexp",
				"punctuation.definition.template-expression",
				"variable.interpolation.scss",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#B97700"
			}
		},
		{
			"scope": [
				"variable",
				"meta.template.expression",
				"constant.other",
				"entity.name.tag.css",
				"entity.name.tag.custom"
			],
			"settings": {
				"foreground": "#212121"
			}
		},
		{
			"scope": [
				"constant",
				"keyword.other.unit",
				"variable.language"
			],
			"settings": {
				"foreground": "#C62828"
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"entity.name.tag.reference"
			],
			"settings": {
				"foreground": "#AC00C0"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#1565C0"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.js",
				"support.class.builtin",
				"support.type",
				"meta.object-literal.key",
				"variable.object.property"
			],
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#9E9E9E",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"token.info-token"
			],
			"settings": {
				"foreground": "#316bcd"
			}
		}
	]
}