{
	"type": "light",
	"colors": {
		"editor.background": "#FAFAFA",
		"editor.foreground": "#212121",
		"editor.lineHighlightBackground": "#01579B11",
		"editor.selectionBackground": "#01579B22",
		"editorCursor.foreground": "#212121",
		"editor.wordHighlightBackground": "#01579B11",
		"editor.wordHighlightStrongBackground": "#01579B22",
		"editor.findMatchHighlightBackground": "#FFA00033",
		"editor.findMatchBackground": "#FFA00077",
		"editor.hoverHighlightBackground": "#01579B22",
		"editorBracketMatch.background": "#01579B22",
		"editorWhitespace.foreground": "#E0E0E0",
		"editorLineNumber.foreground": "#9E9E9E",
		"editorLineNumber.activeForeground": "#78909C",
		"editorWidget.background": "#F5F5F5",
		"editorWidget.border": "#E0E0E0",
		"widget.shadow": "#E0E0E0",
		"editorIndentGuide.activeBackground": "#BDBDBD",
		
		"tab.inactiveBackground": "#F5F5F5",
		"editorGroupHeader.tabsBackground": "#F5F5F5",
		"sideBar.foreground": "#212121",
		"sideBar.background": "#F5F5F5",
		
		"list.hoverBackground": "#01579B11",
		"list.inactiveSelectionBackground": "#01579B22",
		"list.activeSelectionBackground": "#039BE5",
		"list.focusBackground": "#039BE522",
		"editorOverviewRuler.bracketMatchForeground": "#AC00C088",
		"editorOverviewRuler.wordHighlightForeground": "#AC00C088",
		"editorOverviewRuler.wordHighlightStrongForeground": "#AC00C0",
		"editorOverviewRuler.addedForeground": "#2E7D3288",
		"editorGutter.addedBackground": "#2E7D3288",
		"editorOverviewRuler.modifiedForeground": "#1565C088",
		"editorGutter.modifiedBackground": "#1565C088",
		"editorOverviewRuler.deletedForeground": "#C6282888",
		"editorGutter.deletedBackground": "#C6282888",
		
		"statusBar.background": "#0277BD",
		"activityBarBadge.background": "#0277BD",
		"activityBar.foreground": "#F5F5F5",
		"statusBar.foreground": "#FAFAFA",
		"scrollbarSlider.background": "#21212121",
		"input.background": "#FAFAFA",
		"input.foreground": "#212121",
		"input.border": "#E0E0E0"
	},
	"tokenColors": [
		{
			"scope": [
				"string",
				"constant.other.character-class.range.regexp",
				"constant.other.character-class.set.regexp",
				"keyword.operator.quantifier.regexp",
				"punctuation.definition.template-expression",
				"variable.interpolation.scss",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#AF601A"
			}
		},
		{
			"scope": [
				"variable",
				"meta.template.expression",
				"constant.language.pseudo",
				"punctuation.separator.key-value",
				"storage.modifier.import",
				"new.expr entity.name.type.module",
				"constant.other.php",
				"support.variable"
			],
			"settings": {
				"foreground": "#212121"
			}
		},
		{
			"scope": [
				"constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#C62828"
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"entity.name.tag.reference"
			],
			"settings": {
				"foreground": "#AC00C0"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.function",
				"support.function",
				"entity.name.type.class",
				"new.expr entity.name.type",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#1565C0"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"support.class.builtin",
				"meta.object-literal.key",
				"variable.object.property",
				"support.type.property-name.json",
				"entity.name.tag.css",
				"entity.name.tag.custom"
			],
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc",
				"comment.block.documentation.phpdoc"
			],
			"settings": {
				"foreground": "#9E9E9E",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.type.annotation",
				"meta.return.type",
				"keyword.operator.type.annotation",
				"meta.type.parameters",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#78909C"
			}
		},
		{
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#C62828",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.raw",
				"markup.inline.raw"
			],
			"settings": {
				"foreground": "#B97700"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"fontStyle": "bold"
			}
		}
	]
}